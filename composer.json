{
  "name": "plaisio/core",
  "description": "PhpPlaisio: (Development) Core",
  "keywords": [
    "PhpPlaisio",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-curl": "*",
    "plaisio-ts/helper-cast": "^2.1.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/authority-core": "^2.2.0",
    "plaisio/babel-core": "^2.0.0",
    "plaisio/cgi": "^1.2.0",
    "plaisio/db-company": "^1.1.0",
    "plaisio/db-page": "^0.9.2",
    "plaisio/db-profile": "^1.2.0",
    "plaisio/db-user": "^1.3.0",
    "plaisio/exception": "^1.1.0",
    "plaisio/form": "^0.55.5",
    "plaisio/form-louver": "^0.55.2",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/helper-ob": "^1.1.1",
    "plaisio/kernel": "^3.2.1",
    "plaisio/menu": "^1.0.0",
    "plaisio/page-core": "^1.5.0",
    "plaisio/response-core": "^2.8.0",
    "plaisio/table-detail": "^4.0.1",
    "plaisio/table-overview": "^8.1.0",
    "plaisio/web-assets": "^3.0.0",
    "setbased/exception": "^2.3.0",
    "setbased/php-stratum-mysql": "^7.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/console-stratum": "^1.0.1",
    "plaisio/console-assets": "^1.1.0",
    "plaisio/console-type-script": "^1.0.0",
    "plaisio/phing-extensions": "^3.2.0",
    "setbased/php-stratum": "^6.3.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Core\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
