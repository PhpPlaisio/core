{
  "name": "plaisio/core",
  "description": "PhpPlaisio: (Development) Core",
  "keywords": [
    "PhpPlaisio",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-curl": "*",
    "plaisio/babel-core": "^1.0",
    "plaisio/db-company": "^1.0",
    "plaisio/db-profile": "^1.0",
    "plaisio/db-user": "^1.0",
    "plaisio/exception": "^1.0",
    "plaisio/form": "^0.11.0",
    "plaisio/form-louver": "^0.11.0",
    "plaisio/helper-html": "^1.0",
    "plaisio/helper-password": "^1.0",
    "plaisio/kernel": "^1.0",
    "plaisio/page-core": "^1.0",
    "plaisio/response-core": "^1.0",
    "plaisio/table-detail": "^1.0",
    "plaisio/table-overview": "^1.0",
    "setbased/exception": "^2.1",
    "setbased/php-stratum-mysql": "^4.4"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "setbased/php-stratum": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Core\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
