{
  "name": "plaisio/core",
  "description": "PhpPlaisio: (Development) Core",
  "keywords": [
    "PhpPlaisio",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "phpunit/phpunit": "^9.5",
    "plaisio/babel-core": "^1.4",
    "plaisio/cgi": "^1.2",
    "plaisio/db-company": "^1.1",
    "plaisio/db-profile": "^1.1",
    "plaisio/db-user": "^1.1",
    "plaisio/exception": "^1.1",
    "plaisio/form": "9999999-dev",
    "plaisio/form-louver": "9999999-dev",
    "plaisio/helper-html": "^1.4.0",
    "plaisio/helper-ob": "^1.1.1",
    "plaisio/kernel": "^3.1.1",
    "plaisio/menu": "^1.0",
    "plaisio/page-core": "^1.4.2",
    "plaisio/page-decorator": "1.1",
    "plaisio/response-core": "^2.3",
    "plaisio/table-detail": "^1.3.1",
    "plaisio/table-overview": "^3.0.3",
    "plaisio/web-assets": "^2.0",
    "setbased/exception": "^2.1.2",
    "setbased/php-stratum-mysql": "^6.3.0"
  },
  "require-dev": {
    "phing/phing": "^2.16.3",
    "plaisio/console": "^2.4.8",
    "setbased/php-stratum": "^6.1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Core\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
