{
  "name": "setbased/abc-core",
  "description": "ABC-Framework: (Development) Core",
  "keywords": [
    "ABC-Framework",
    "Core"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "gajus/dindent": "^2.0.0",
    "setbased/abc": "*",
    "setbased/abc-babel-core": "*",
    "setbased/abc-company": "^1.1.0",
    "setbased/abc-exception": "^1.0.0",
    "setbased/abc-form": "*",
    "setbased/abc-form-louver": "*",
    "setbased/abc-helper-password": "^2.0.0",
    "setbased/abc-html": "^2.0.0",
    "setbased/abc-http-header": "^2.0.0",
    "setbased/abc-page-core": "*",
    "setbased/abc-profile": "^1.1.0",
    "setbased/abc-table-detail": "*",
    "setbased/abc-table-overview": "*",
    "setbased/abc-user": "^1.1.0",
    "setbased/exception": "^1.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "setbased/php-stratum": "^4.0.0",
    "setbased/php-stratum-mysql": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Core\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
